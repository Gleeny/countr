{
  "name": "countr",
  "main": "build/bot.js",
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc",
    "docker": "docker-compose up --build",
    "docker:clusterfuck": "docker-compose -f docker/docker-compose-clusterfuck.yml up --build",
    "docker:no-db": "docker-compose -f docker/docker-compose-no-db.yml up --build",
    "docker:no-manager": "docker-compose -f docker/docker-compose-no-manager.yml up --build",
    "docker:standalone": "docker-compose -f docker/docker-compose-standalone.yml up --build",
    "lint": "eslint ./src",
    "manager": "node ./build/manager",
    "start": "node .",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.3.1",
    "bufferutil": "^4.0.5",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "erlpack": "github:discord/erlpack",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "match-sorter": "^6.3.1",
    "mathjs": "^10.0.0",
    "mongoose": "^6.0.14",
    "roman-numerals": "^0.3.2",
    "superagent": "^6.1.0",
    "time-limited-regular-expressions": "^1.0.0",
    "utf-8-validate": "^5.0.7",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/roman-numerals": "^0.3.0",
    "@types/superagent": "^4.1.13",
    "@types/time-limited-regular-expressions": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1"
  }
}
